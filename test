#include <stdio.h>
#include <time.h>
#include <stdlib.h>

void fill_array(int **array, int row, int col) {
	srand(time(NULL));
	for (int i = 0; i < row; ++i) {
		for (int j = 0; j < col; ++j) {
			array[i][j] = rand() % 2;
		}
	}
}

void print_array(int **array, int row, int col) {
	for (int i = 0; i < row; ++i) {
		for (int j = 0; j < col; ++j) {
			printf("%d ", array[i][j]);
		}
		printf("\n");
	}
}

int main() {
	int row = 5;
	int col = 5;
	int arr[row][col];

	fill_array(arr, row, col);
	print_array(arr, row, col);

	return 0;
}
